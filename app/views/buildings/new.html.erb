<%= simple_form_for @building do |f| %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state, collection: States.list %>
  <%= f.input :postal_code, label: 'Postal Code' %>
  <%= f.input :description %>
  <%= f.input :owner_id, label: 'Owner', collection: Owner.all, label_method: :display_string %>
  <%= f.button :submit %>
<% end %>
